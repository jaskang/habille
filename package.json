{
  "name": "habille",
  "version": "0.0.0",
  "description": "A starter for creating a React component library.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/jaskang/habille#readme",
  "bugs": {
    "url": "https://github.com/jaskang/habille/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaskang/habille.git"
  },
  "author": "jaskang<jaskang@gmail.com>",
  "funding": "https://github.com/sponsors/jaskang",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@jaskang/config": "^2.5.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.4",
    "@vitejs/plugin-react": "^4.4.1",
    "bumpp": "^10.1.0",
    "eslint": "^9.26.0",
    "happy-dom": "^17.4.6",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsdown": "^0.11.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@10.10.0"
}
